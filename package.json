{
  "name": "fip-client",
  "version": "0.0.0",
  "description": "HTML5 Player for the awesome FIP Radio",
  "main": "./src/js/index.jsx",
  "keywords": [
    "fip",
    "fipradio",
    "radio",
    "player",
    "html5",
    "music"
  ],
  "author": "Rodolphe Belouin <me@rbelouin.com>",
  "license": "MIT",
  "engines": {
    "node": 6
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-react-jsx": "^6.0.18",
    "babel-preset-es2015": "^6.1.2",
    "baconjs": "^0.7.74",
    "bootstrap": "^3.3.5",
    "common-env": "^5.2.0",
    "eslint": "^4.1.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.13.3",
    "glob": "^7.1.2",
    "gulp": "^3.9.0",
    "gulp-less": "^3.0.3",
    "lodash": "^3.10.1",
    "tap-webpack-plugin": "^2.0.0",
    "tape": "^4.2.1",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "bacon-routes": "^1.0.0",
    "baconjs": "^0.7.74",
    "create-react-class": "^15.6.0",
    "font-awesome": "^4.4.0",
    "intl": "^1.2.5",
    "lodash": "^3.10.1",
    "promise": "^7.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-intl": "^2.3.0",
    "uuid": "^2.0.1",
    "whatwg-fetch": "^0.10.1"
  },
  "scripts": {
    "build": "gulp build && webpack",
    "install": "gulp build && webpack",
    "start": "node ./prod/js/server.js",
    "test": "webpack",
    "watch": "gulp watch"
  },
  "babel": {
    "plugins": [
      "transform-react-jsx"
    ],
    "presets": [
      "es2015"
    ],
    "ignore": [
      "node_modules"
    ]
  },
  "common_env": {
    "api": {
      "http_host": "http://fip.rbelouin.com/api",
      "ws_host": "ws://fip.rbelouin.com/api/ws"
    },
    "stats-api": {
      "http_host": "https://fip-stats.cleverapps.io/api"
    },
    "default_language": "en",
    "port": 8080,
    "radios": [
      {
        "name": "fip-radio",
        "src": "http://direct.fipradio.fr/live/fip-midfi.mp3"
      },
      {
        "name": "fip-autour-du-rock",
        "src": "http://direct.fipradio.fr/live/fip-webradio1.mp3"
      },
      {
        "name": "fip-autour-du-jazz",
        "src": "http://direct.fipradio.fr/live/fip-webradio2.mp3"
      },
      {
        "name": "fip-autour-du-groove",
        "src": "http://direct.fipradio.fr/live/fip-webradio3.mp3"
      },
      {
        "name": "fip-autour-du-monde",
        "src": "http://direct.fipradio.fr/live/fip-webradio4.mp3"
      },
      {
        "name": "tout-nouveau-tout-fip",
        "src": "http://direct.fipradio.fr/live/fip-webradio5.mp3"
      },
      {
        "name": "fip-il-y-45-ans",
        "src": "http://direct.fipradio.fr/live/fip-webradio6.mp3"
      }
    ],
    "routes": {
      "favorites": "/users/me/songs",
      "radio": "/radios/:radio",
      "home": "/"
    },
    "spotify": {
      "api_host": "https://api.spotify.com/v1"
    },
    "storage": {
      "keys": {
        "favorites": "favorites",
        "token": "token"
      }
    }
  }
}
